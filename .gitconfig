[user]
	name = Hansen Zhang
	email = hansen@fulcrumpro.com 
[core]
	excludesfile = /Users/hansen/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	co = checkout
	last = log -1 HEAD
	dp = checkout Deployments/Production
	rc = checkout Deployments/RC
	m = checkout master
	ol = log --oneline
